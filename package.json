{
  "type": "module",
  "name": "@dependable/view",
  "version": "0.5.0",
  "description": "View layer for @dependable/state",
  "sideEffects": false,
  "module": "./src/state.js",
  "exports": {
    ".": "./src/index.js",
    "./h": "./src/h.js",
    "./html": "./src/html.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "dependable",
    "vdom",
    "view",
    "reactive",
    "observable",
    "computed",
    "state"
  ],
  "scripts": {
    "test": "mocha",
    "format": "prettier --write '**/*.js'",
    "lint": "prettier --check '**/*.js' && eslint .",
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "coverage": "c8 -r lcov -r text npm test",
    "prepublishOnly": "yarn build"
  },
  "author": "Sune Simonsen",
  "license": "MIT",
  "bugs": "https://github.com/sunesimonsen/dependable-view/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunesimonsen/dependable-view"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@dependable/state": "^0.5.0",
    "htm": "^3.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fake-promise": "^2.5.44",
    "mocha": "^10.0.0",
    "mocha-dominate": "^2.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.27.1",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^14.0.0",
    "terser": "^5.14.0",
    "unexpected": "^13.0.0",
    "unexpected-check": "^3.1.0",
    "unexpected-dom": "^5.1.0",
    "unexpected-sinon": "^11.1.0"
  }
}
